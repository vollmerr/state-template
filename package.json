{
  "name": "state-template",
  "version": "0.3.3",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "sideEffects": false,
  "files": [
    "dist",
    "scripts/fixPikaday.js",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "URL_OF_YOUR_REPOSITORY"
  },
  "scripts": {
    "prebuild": "cross-env rimraf dist && npm run update:icons",
    "build": "cross-env NODE_ENV=production babel src/lib --out-dir dist --copy-files",
    "test": "react-scripts test --coverage --watchAll=false",
    "test:watch": "react-scripts test",
    "visual": "backstop test",
    "visual:update": "backstop approve",
    "docs": "styleguidist server",
    "docs:build": "styleguidist build",
    "prepare": "npm run build",
    "postinstall": "node ./scripts/fixPikaday.js",
    "update:icons": "node ./scripts/updateIconList.js"
  },
  "peerDependencies": {
    "react": "16.x",
    "react-dom": "16.x",
    "react-redux": "7.x",
    "react-router-dom": "5.x",
    "redux": "4.x",
    "redux-actions": "2.x",
    "redux-form": "8.x",
    "redux-saga": "1.x",
    "reselect": "4.x"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/runtime": "^7.4.5",
    "@types/history": "^4.7.2",
    "@types/jest": "^24.0.13",
    "@types/node": "^12.0.7",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "backstopjs": "^4.0.6",
    "cross-env": "^5.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.3.5",
    "eslint-config-airbnb": "^17.1.0",
    "faker": "^4.1.0",
    "lint-staged": "^8.2.0",
    "mockdate": "^2.0.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.3",
    "react-router-dom": "^5.0.1",
    "react-scripts": "^3.0.1",
    "react-styleguidist": "^9.1.5",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.5",
    "redux-form": "^8.2.3",
    "redux-saga": "^1.0.3",
    "reselect": "^4.0.0",
    "rimraf": "^2.6.3",
    "typescript": "^3.5.1"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "history": "^4.9.0",
    "jwt-decode": "^2.2.0",
    "lodash.debounce": "^4.0.8",
    "node-sass": "^4.12.0",
    "pikaday": "^1.8.0",
    "prop-types": "^15.7.2",
    "react-bootstrap-table-next": "^3.1.4",
    "react-bootstrap-table2-paginator": "^2.0.6",
    "react-bootstrap-table2-toolkit": "^2.0.1",
    "react-tooltip": "^3.10.0",
    "redux-devtools-extension": "^2.13.8"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "resetMocks": true,
    "collectCoverageFrom": [
      "src/lib/**/*.{js,jsx}",
      "!src/lib/style/**/*.{js,jsx}"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
